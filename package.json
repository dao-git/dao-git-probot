{
  "name": "dao-git-probot",
  "version": "1.0.0",
  "description": "A Probot app",
  "author": "dcruwys <dcruwys@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com//dao-git-probot.git",
  "homepage": "https://github.com//dao-git-probot",
  "bugs": "https://github.com//dao-git-probot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard index.js test/*.js",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@octokit/rest": "^16.15.0",
    "octokit": "^1.0.0-hello-world",
    "probot": "^7.2.0",
    "twitter": "^1.7.1",
    "web3": "^1.0.0-beta.46"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
